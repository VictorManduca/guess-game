{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"outDir": "./dir",
		"strict": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"strictPropertyInitialization": false
	}
}
